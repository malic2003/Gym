<h1>Listing proveedors</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Tel casa</th>
    <th>Tel particular</th>
    <th>Direccion</th>
    <th>Mail</th>
    <th>Notas</th>
    <th>Cuil</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proveedors.each do |proveedor| %>
  <tr>
    <td><%= proveedor.nombre %></td>
    <td><%= proveedor.apellido %></td>
    <td><%= proveedor.tel_casa %></td>
    <td><%= proveedor.tel_particular %></td>
    <td><%= proveedor.direccion %></td>
    <td><%= proveedor.mail %></td>
    <td><%= proveedor.notas %></td>
    <td><%= proveedor.cuil %></td>
    <td><%= link_to 'Show', proveedor %></td>
    <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
    <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Proveedor', new_proveedor_path %>
