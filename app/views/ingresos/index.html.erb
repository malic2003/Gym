<h1>Listing ingresos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Monto</th>
    <th>Turno</th>
    <th>Bar</th>
    <th>Paleta</th>
    <th>Prot grip</th>
    <th>Pelota</th>
    <th>Fecha</th>
    <th>Cliente</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ingresos.each do |ingreso| %>
  <tr>
    <td><%= ingreso.nombre %></td>
    <td><%= ingreso.descripcion %></td>
    <td><%= ingreso.monto %></td>
    <td><%= ingreso.turno %></td>
    <td><%= ingreso.bar %></td>
    <td><%= ingreso.paleta %></td>
    <td><%= ingreso.prot_grip %></td>
    <td><%= ingreso.pelota %></td>
    <td><%= ingreso.fecha %></td>
    <td><%= ingreso.cliente %></td>
    <td><%= link_to 'Show', ingreso %></td>
    <td><%= link_to 'Edit', edit_ingreso_path(ingreso) %></td>
    <td><%= link_to 'Destroy', ingreso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ingreso', new_ingreso_path %>
